{
    "name": "danhunsaker/laravel-topology",
    "description": "A Laravel package for password-topology-check",
    "keywords": [
        "laravel",
        "laravel5",
        "laravel 5",
        "validation",
        "password",
        "password topology"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hunsaker",
            "email": "danhunsaker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "danhunsaker/password-topology-check": "dev-master",
        "illuminate/support": "^5.6 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "DanHunsaker\\PasswordTopology\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanHunsaker\\PasswordTopology\\TopologyServiceProvider"
            ]
        }
    }
}
